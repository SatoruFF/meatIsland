{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Продукты",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "description": {
      "type": "text"
    },
    "available": {
      "type": "boolean"
    },
    "weight": {
      "type": "float"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "StockQuantity": {
      "type": "integer"
    },
    "sale": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sale.sale",
      "inversedBy": "products"
    },
    "tradePrice": {
      "type": "float"
    },
    "recommendation": {
      "type": "boolean",
      "default": false
    }
  }
}
